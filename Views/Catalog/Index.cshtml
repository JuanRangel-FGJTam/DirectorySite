@model IEnumerable<DirectorySite.Models.Occupation>?
@{
    ViewData["Title"] = "Catalog Page";
}

<div class="text-center">
    <h1 class="display-4">Catalogs</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item active" aria-current="page">Catalogs</li>
  </ol>
</nav>

<div class="fluid-container ">
    <div class="row align-items-start">
        <div class="col-2">
            @Html.Partial("./Partials/CatalogSidebar" )
        </div>
        <div class="col-10">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                    <th scope="col">#</th>
                    <th scope="col">Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>

                </tbody>
            </table>
        </div>
    </div>
</div>