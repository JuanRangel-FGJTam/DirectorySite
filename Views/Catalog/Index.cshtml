@model IEnumerable<DirectorySite.Models.Occupation>?
@{
    ViewData["Title"] = "Catalogos";
}

<partial name="./Partials/PageHeader" />

<div class="fluid-container ">
    <div class="row align-items-start">
        <div class="col-2">
            <partial name="./Partials/CatalogSidebar" />
        </div>
        <div class="col-10 border rounded">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                    <th scope="col">#</th>
                    <th scope="col">Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td colspan="4">
                            <div class="text-center text-secondary py-4">
                                <span>Seleccione un elemento</span>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>