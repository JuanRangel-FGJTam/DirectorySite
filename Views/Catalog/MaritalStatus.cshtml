@model IEnumerable<DirectorySite.Models.MaritalStatus>?
@{
    ViewData["Title"] = "Catalog Page";
}

<div class="text-center">
    <h1 class="display-4">Catalogs</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Catalog">Catalogs</a></li>
    <li class="breadcrumb-item active" aria-current="page">Nationalities</li>
  </ol>
</nav>

<div class="fluid-container ">
    <div class="row align-items-start">
        <div class="col-2">
            @Html.Partial("./Partials/CatalogSidebar")
        </div>

        <div class="col-10">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                    <th scope="col">#</th>
                    <th scope="col">Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @if( Model != null){
                        var i = 1;
                        foreach( var item in Model! ){
                            <tr>
                                <th scope="row">@i</th>
                                <td>@item.Id</td>
                                <td>@item.Name</td>
                                <td>
                                    <a href="#" class="btn btn-primary btn-sm px-4" tabindex="-1" role="button" aria-disabled="true">Show</a>
                                    <a href="#" class="btn btn-primary btn-sm px-4" tabindex="-1" role="button" aria-disabled="true">Edit</a>
                                </td>
                            </tr>
                            i++;
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>